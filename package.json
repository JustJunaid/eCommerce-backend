{
    "name": "nest-typescript",
    "version": "0.0.1",
    "description": "Nest + JWT + Jest + Swagger",
    "keywords": [
        "nest",
        "nestjs",
        "nest.js",
        "orm",
        "nodejs",
        "node.js",
        "node",
        "typescript",
        "jwt",
        "jsonwebtoken",
        "passport",
        "swagger",
        "jest"
    ],
    "author": "Kent Loog",
    "license": "MIT",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "format": "prettier --write \"src/**/*.ts\"",
        "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed-dev && npm run db:seed-prod",
        "start": "ts-node -r tsconfig-paths/register src/main.ts",
        "start:dev": "nodemon",
        "start:debug": "nodemon --config nodemon-debug.json",
        "prestart:prod": "rimraf dist && npm run build",
        "start:prod": "node dist/src/main.js",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "test": "jest --config ./test/jest-e2e.json",
        "docker:db": "docker-compose -f docker-compose.db.yml up -d",
        "migrate:dev": "prisma migrate dev --preview-feature",
        "seed": "prisma db seed --preview-feature"
    },
    "dependencies": {
        "@nestjs-modules/mailer": "^1.6.0",
        "@nestjs/common": "^7.0.1",
        "@nestjs/config": "^1.0.1",
        "@nestjs/core": "^7.0.1",
        "@nestjs/graphql": "^9.0.4",
        "@nestjs/passport": "^7.0.0",
        "@nestjs/platform-express": "^7.0.1",
        "@nestjs/swagger": "^4.4.0",
        "@prisma/client": "^2.30.0",
        "apollo-server-express": "^3.3.0",
        "bcrypt": "^4.0.1",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.11.0",
        "graphql": "^15.5.2",
        "handlebars": "^4.7.7",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "nest": "^0.1.6",
        "nodemailer": "^6.6.3",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.3.0",
        "swagger-ui-express": "^4.1.6"
    },
    "devDependencies": {
        "@nestjs/testing": "^7.0.1",
        "@types/bcrypt": "^3.0.0",
        "@types/express": "^4.17.3",
        "@types/jest": "^25.1.4",
        "@types/jsonwebtoken": "^8.3.8",
        "@types/node": "^13.9.1",
        "@types/nodemailer": "^6.4.4",
        "@types/passport": "^1.0.3",
        "@types/passport-jwt": "^3.0.3",
        "@types/supertest": "^2.0.8",
        "jest": "^25.1.0",
        "nodemon": "^2.0.2",
        "prettier": "^1.19.1",
        "prisma": "^2.30.2",
        "prisma-dbml-generator": "^0.7.0",
        "supertest": "^4.0.2",
        "ts-jest": "^25.2.1",
        "ts-node": "^8.6.2",
        "tsconfig-paths": "^3.9.0",
        "tslint": "6.1.0",
        "typescript": "^3.8.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
